{
  "name": "maatify/data-fakes",
  "description": "Complete fake simulation layer for MySQL, DBAL, Redis, and Mongo adapters.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Maatify\\DataFakes\\": "src/"
    }
  },
  "require": {
    "php": ">=8.4",
    "maatify/common": "^1.0",
    "maatify/data-adapters": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "scripts": {
    "analyse": "phpstan analyse src tests --level=max",
    "test": "phpunit",
    "format": "php-cs-fixer fix"
  }
}
